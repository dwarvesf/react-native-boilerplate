{
  "name": "rnboiler",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "c": "code . && open ios/rnboiler.xcworkspace && react-native start && open -a Reactotron.app",
    "xcode": "open ios/rnboiler.xcworkspace",
    "studio": "open -a /Applications/Android\\ Studio.app ./android/",
    "dev": "react-native start",
    "adb": "adb reverse tcp:8081 tcp:8081",
    "android ram": "adb shell dumpsys meminfo",
    "assets-link": "npx react-native link",
    "ip": "ifconfig | grep 'inet '",
    "gen pem": "openssl pkcs12 -in certi.p12 -out certi.pem -nodes -clcerts",
    "devices": "xcrun simctl list devices",
    "android-menu": "cd and && adb shell input keyevent 82",
    "android-release": "clean-android && npx react-native run-android --variant=release",
    "pod": "cd ios && pod install && cd ..",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-ios": "cd ios && xcodebuild clean && cd ..",
    "devtools": "react-devtools",
    "dev clean": "npx react-native start --reset-cache",
    "apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "build-android": "cd android && ./gradlew assembleRelease&& adb -d install app/build/outputs/apk/release/app-release.apk",
    "bundle-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "open-folder-apk": "open ./android/app/build/outputs/apk",
    "android-run-debug-develop": "react-native run-android --no-jetifier --variant=devDebug --appId 'com.rnboiler.dev'",
    "android-run-debug-staging": "react-native run-android --no-jetifier --variant=stagingDebug --appId 'com.rnboiler.stg'",
    "android-run-debug-product": "react-native run-android --no-jetifier --variant=productDebug --appId 'com.rnboiler.prod'",
    "android-run-release-develop": "cd android && rm -rf build && rm -rf android/build && cd .. && react-native run-android --no-jetifier --variant=devRelease --appId 'com.rnboiler.dev'",
    "android-run-release-staging": "react-native run-android --no-jetifier --variant=stagingRelease --appId 'com.rnboiler.stg'",
    "android-run-release-product": "react-native run-android --no-jetifier --variant=productRelease --appId 'com.rnboiler.prod'",
    "android-build-apk-develop": "rm -rf build && rm -rf android/build && export ENVFILE=.env.development && cd android && ./gradlew assembleDevRelease && cd ..",
    "android-build-apk-staging": "rm -rf build && rm -rf android/build && export ENVFILE=.env.staging && cd android && ./gradlew assembleStagingRelease && cd ..",
    "android-build-apk-product": "rm -rf build && rm -rf android/build && export ENVFILE=.env.production && cd android && ./gradlew clean && ./gradlew assembleProductRelease && cd ..",
    "android-build-aab-product": "rm -rf build && rm -rf android/build && export ENVFILE=.env.production && cd android && ./gradlew clean && ./gradlew bundleProductRelease && cd ..",
    "code-push-ios-develop": "appcenter codepush release-react -a Apps/project-name -d Development -t 1.0.0",
    "code-push-ios-staging": "appcenter codepush release-react -a Apps/project-name -d Staging -t 1.0.0",
    "code-push-ios-production": "appcenter codepush release-react -a Apps/project-name -d Production -t 1.0.0",
    "code-push-android-develop": "appcenter codepush release-react -a Apps/project-name -d Development -t 1.0.0",
    "code-push-android-staging": "appcenter codepush release-react -a Apps/project-name -d Staging -t 1.0.0",
    "code-push-android-production": "appcenter codepush release-react -a Apps/project-name -d Production -t 1.0.0",
    "push-develop": "yarn code-push-ios-develop && yarn code-push-android-develop",
    "push-staging": "yarn code-push-ios-staging && yarn code-push-android-staging",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher"
  },
  "dependencies": {
    "@dwarvesf/react-hooks": "^0.7.0",
    "@dwarvesf/react-utils": "^0.4.1",
    "@expo-google-fonts/inter": "^0.2.3",
    "@gorhom/bottom-sheet": "^4",
    "@hookform/resolvers": "^2.9.7",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-community/blur": "^4.2.0",
    "@react-native-community/cameraroll": "^4.1.2",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/datetimepicker": "^7.0.1",
    "@react-native-community/netinfo": "^9.3.0",
    "@react-native-masked-view/masked-view": "^0.2.7",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@react-navigation/stack": "^6.3.16",
    "@shopify/flash-list": "^1.3.1",
    "dayjs": "^1.11.7",
    "isomorphic-unfetch": "^3.1.0",
    "lottie-react-native": "^5.1.4",
    "numeral": "^2.0.6",
    "query-string": "^7.1.1",
    "react": "18.2.0",
    "react-hook-form": "^7.34.2",
    "react-i18next": "^11.18.3",
    "react-native": "0.71.6",
    "react-native-calendars": "^1.1288.3",
    "react-native-code-push": "^7.0.5",
    "react-native-config": "^1.4.6",
    "react-native-date-picker": "^4.2.5",
    "react-native-device-info": "^10.0.2",
    "react-native-dropdownalert": "^4.5.1",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-mmkv": "^2.4.3",
    "react-native-modal-datetime-picker": "^14.0.0",
    "react-native-modalbox": "^2.0.2",
    "react-native-reanimated": "2.10.0",
    "react-native-responsive-dimensions": "^3.1.1",
    "react-native-safe-area-context": "^4.5.1",
    "react-native-screens": "^3.15.0",
    "react-native-shimmer-placeholder": "^2.0.9",
    "react-native-simple-toast": "^1.1.4",
    "react-native-tab-view": "^3.3.0",
    "react-native-uuid": "^2.0.1",
    "react-native-vision-camera": "^2.14.1",
    "react-native-webview": "^11.23.1",
    "swr": "^2.1.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native-community/slider": "^4.4.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-ondevice-actions": "^6.5.3",
    "@storybook/addon-ondevice-controls": "^6.5.3",
    "@storybook/react-native": "^6.5.3",
    "@tsconfig/react-native": "^2.0.2",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^29.2.1",
    "@types/numeral": "^2.0.2",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.19.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-jsconfig": "^1.1.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.0.2",
    "reactotron-redux": "^3.1.3",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
